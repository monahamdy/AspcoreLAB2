
@{
    ViewData["Title"] = "ShowTraineeResult";
}

<h1>ShowTraineeResult</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th scope="col">Name</th>
            <th scope="col">Grade</th>



        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr style="color:@item.Color">
                <td>@item.CrsName</td>
                <td>
                    @item.TraineeGrade
                </td>
            </tr>
        }
    </tbody>
</table>