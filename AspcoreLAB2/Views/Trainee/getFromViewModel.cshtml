@model AspcoreLAB2.ViewModel.TraineeCourseCrsResultViewModel;
@{
    ViewData["Title"] = "getFromViewModel";
}




<div class="container">
    <section class="mx-auto my-5" style="max-width: 23rem;">

        <div class="card">
            <div class="card-body d-flex flex-row">
               
                <div style="color:@Model.Color;">
                    <h5 class="card-title font-weight-bold mb-2 text-capitalize"> Trainee Name : @Model.TraineeName</h5>
                    <p class="card-text"><i class="far fa-clock pe-2"></i>Coures : @Model.CrsName</p>
                    <p class="card-text"><i class="far fa-clock pe-2"></i>Grade : @Model.TraineeGrade</p>


                </div>
            </div>
        </div>

    </section>
</div>